{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/(auth)/sign-up/[[...sign-up]]/page.jsx", "app/(auth)/sign-in/[[...sign-in]]/page.jsx", "app/(root)/page.jsx", "app/providers.jsx", "components/Header.jsx", "components/Header.jsx", "app/pantry/page.jsx", "app/pantry/ResponseDetailsModal.jsx", "app/pantry/page.jsx", "app/pantry/Columns.jsx", "app/pantry/DataTable.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
